{
  "author": {
    "name": "l-you",
    "email": "l-you@revotale.com",
    "url": "https://github.com/l-you"
  },
  "version": "0.6.18",
  "name": "eslint-config-grisaia-typescript",
  "main": "src/index.ts",
  "license": "MIT",
  "private": false,
  "sideEffects": false,
  "publishConfig": {
    "provenance": true,
    "access": "public",
    "main": "index.js",
    "directory": "dist",
    "exports": {
      "./package.json": "./package.json",
      ".": {
        "import": "./index.js",
        "require": "./index.cjs",
        "types": "./index.d.ts"
      }
    }
  },
  "type": "module",
  "scripts": {
    "build": "bun ../package-builder/dist/bin/cli.js . && tsc",
    "types:check": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RevoTale/js-package-utils.git"
  },
  "peerDependencies": {
    "eslint": "^8.44.2",
    "@typescript-eslint/eslint-plugin": "^6.6.0 || ^7.0.0"
  },
  "devDependencies": {
    "@types/eslint": "^9.6.1"
  },
  "bugs": {
    "url": "https://github.com/RevoTale/js-package-utils/issues"
  }
}
