  },
  "version": "0.5.0",
  "name": "eslint-config-grisaia-typescript",
  "main": "index.js",
  "license": "MIT",
  "private": false,
  "sideEffects": false,
  "exports": {
    "./package.json": "./package.json",
    "./index.js": "./index.js"
  },
  "publishConfig": {
    "provenance": true,
    "access": "public"
  },
  "type": "module",
  "scripts": {
    "build:types": "tsc --emitDeclarationOnly",
    "build:copy": "cp package.json dist/ && cp README.md dist/ && cp LICENSE dist/",
    "build:code": "node ./build.js",
    "build": "pnpm build:code && pnpm build:types && pnpm build:copy",
    "types:check": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RustedLabs/eslint-config.git"
  },
  "peerDependencies": {
    "eslint": "^7.32.0 || ^8.2.0",
    "@typescript-eslint/eslint-plugin": "^6.6.0"
  },
  "devDependencies": {
    "@types/eslint": "^8.44.2"
  },
  "bugs": {
    "url": "https://github.com/RustedLabs/eslint-config/issues"
  }
}
