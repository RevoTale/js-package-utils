{
  "author": {
    "name": "Liakhovskyi Vladyslav",
    "email": "vlad@rusted-labs.org",
    "url": "https://github.com/rusted-love"
  },
  "version": "0.6.2",
  "name": "eslint-config-grisaia-typescript",
  "main": "src/index.ts",
  "license": "MIT",
  "private": false,
  "sideEffects": false,
  "publishConfig": {
    "provenance": true,
    "access": "public",
    "main": "index.js",
    "directory": "dist",
    "exports": {
      "./package.json": "./package.json",
      ".": {
        "import": "./index.js",
        "require": "./index.cjs",
        "types": "./index.d.ts"
      }
    }
  },
  "type": "module",
  "scripts": {
    "build": "bun ../build/src/index.ts . && tsc",
    "types:check": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RustedLabs/eslint-config.git"
  },
  "peerDependencies": {
    "eslint": "^8.44.2",
    "@typescript-eslint/eslint-plugin": "^6.6.0"
  },
  "devDependencies": {
    "@types/eslint": "^8.44.2"
  },
  "bugs": {
    "url": "https://github.com/RustedLabs/eslint-config/issues"
  }
}
