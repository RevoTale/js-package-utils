{
  "name": "grisaia-package-builder",
  "version": "0.1.8",
  "dependencies": {},
  "type": "module",
  "author": {
    "name": "Liakhovskyi Vladyslav",
    "email": "vlad@rusted-labs.org",
    "url": "https://github.com/rusted-love"
  },
  "main": "src/index.ts",
  "scripts": {
    "build": "bun ./src/bin/cli.ts . && tsc"
  },
  "publishConfig": {
    "provenance": true,
    "access": "public",
    "main": "index.js",
    "directory": "dist",
    "exports": {
      "./package.json": "./package.json",
      ".": {
        "import": "./index.js",
        "require": "./index.cjs",
        "types": "./index.d.ts"
      }
    },
    "bin": {
      "gbuild": "./bin/cli.cjs"
    }
  },
  "private": false,
  "sideEffects": false,
  "bugs": {
    "url": "https://github.com/RustedLabs/js-package-utils/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RustedLabs/js-package-utils.git"
  }
}
